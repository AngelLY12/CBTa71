---
import NavigationSideBar from "./NavigationSideBar.astro";
import Title from "./Title.astro";

interface Props {
    title: string
}

const {title} = Astro.props
---

<div class="flex relative">
    <NavigationSideBar />
    <div class="lg:pl-44 lg:pr-6 px-2 pt-4 w-full">
        <Title title={title} /> 
        <slot></slot>
    </div>
</div>
